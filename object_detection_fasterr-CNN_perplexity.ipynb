    "plt.xlabel('Epoch')\n",
    "plt.ylabel('Loss')\n",
    "plt.title('Training and Validation Loss')\n",
    "plt.legend()\n",
    "plt.savefig(os.path.join(WORKING_DIR, 'loss_plot.png'))\n",
    "plt.close()\n",
